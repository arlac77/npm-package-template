{
  "name": "{{name}}",
  "version": "0.0.0-semantic-release",
  "description": "{{description}}",
  "module": "{{module}}",
  "main": "{{main}}",
  "publishConfig": {
    "access": "public"
  },
  "keywords": ["npm-package-template"],
  "scripts": {
    "cover": "nyc --temp-directory build/nyc ava",
    "precover": "rollup -c tests/rollup.config.js",
    "test": "ava",
    "pretest": "rollup -c tests/rollup.config.js",
    "posttest": "npm run prepare && markdown-doctest",
    "prepare": "rollup -c",
    "docs": "documentation readme {{module}} --section=API",
    "lint": "documentation lint {{module}}",
    "semantic-release": "semantic-release",
    "travis-deploy-once": "travis-deploy-once"
  },
  "dependencies": {},
  "devDependencies": {
    "documentation": "^8.1.0",
    "markdown-doctest": "^0.9.1",
    "nyc": "^13.0.1",
    "ava": "^0.25.0",
    "rollup": "^0.64.1",
    "rollup-plugin-commonjs": "^9.1.5",
    "rollup-plugin-cleanup": "^3.0.0",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-babel": "^3.0.7",
    "rollup-plugin-json": "^3.0.0",
    "rollup-plugin-multi-entry": "^2.0.2",
    "rollup-plugin-istanbul": "^2.0.1",
    "rollup-plugin-executable": "^1.3.0",
    "babel-preset-env": "^1.6.1",
    "semantic-release": "^15.9.6",
    "travis-deploy-once": "^5.0.2"
  },
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "engines": {
    "node": ">=8.11"
  },
  "xo": {
    "space": true
  },
  "ava": {
    "files": ["build/*-test.js"],
    "presets": ["env"],
    "require": ["babel-register"]
  },
  "nyc": {
    "include": ["build/*-test.js"],
    "reporter": ["lcov", "text"],
    "report-dir": "./build/coverage"
  },
  "template": {
    "files": [
      {
        "merger": "Package",
        "pattern": "**/package.json",
        "options": {
          "keywords": {
            "^kronos-interceptor.+": "kronos-interceptor",
            "^kronos-service-(?!manager)": "kronos-service",
            "^kronos-step.+": "kronos-step",
            "^kronos-adapter.+": "kronos-step"
          },
          "actions": [
            {
              "path": "$.nyc['report-dir']",
              "op": "replace"
            }
          ]
        }
      },
      {
        "merger": "Travis",
        "pattern": ".travis.yml"
      },
      {
        "merger": "Readme",
        "pattern": "README.md",
        "options": {
          "badges": [
            {
              "name": "npm",
              "icon": "https://img.shields.io/npm/v/{{name}}.svg",
              "url": "https://www.npmjs.com/package/{{name}}"
            },
            {
              "name": "Greenkeeper",
              "icon":
                "https://badges.greenkeeper.io/{{github.user}}/{{github.repo}}.svg",
              "url": "https://greenkeeper.io/"
            },
            {
              "name": "semantic-release",
              "icon":
                "https://img.shields.io/badge/%20%20%F0%9F%93%A6%F0%9F%9A%80-semantic--release-e10079.svg",
              "url": "https://github.com/{{github.user}}/{{github.repo}}"
            },
            {
              "name": "styled with prettier",
              "icon":
                "https://img.shields.io/badge/styled_with-prettier-ff69b4.svg",
              "url": "https://github.com/prettier/prettier"
            },
            {
              "name": "Build Status",
              "icon":
                "https://secure.travis-ci.org/{{github.user}}/{{github.repo}}.png",
              "url": "http://travis-ci.org/{{github.user}}/{{github.repo}}"
            },
            {
              "name": "install size",
              "icon":
                "https://packagephobia.now.sh/badge?p={{name}}/@{{version}}",
              "url":
                "https://packagephobia.now.sh/result?p={{name}}@{{version}}"
            },
            {
              "name": "codecov.io",
              "icon":
                "http://codecov.io/github/{{github.user}}/{{github.repo}}/coverage.svg?branch=master",
              "url":
                "http://codecov.io/github/{{github.user}}/{{github.repo}}?branch=master"
            },
            {
              "name": "Coverage Status",
              "icon":
                "https://coveralls.io/repos/{{github.user}}/{{github.repo}}/badge.svg",
              "url": "https://coveralls.io/r/{{github.user}}/{{github.repo}}"
            },
            {
              "name": "Maintainability",
              "icon":
                "https://api.codeclimate.com/v1/badges/{{id}}/maintainability",
              "url":
                "https://codeclimate.com/github/{{github.user}}/{{github.repo}}/maintainability"
            },
            {
              "name": "Known Vulnerabilities",
              "icon":
                "https://snyk.io/test/github/{{github.user}}/{{github.repo}}/badge.svg",
              "url":
                "https://snyk.io/test/github/{{github.user}}/{{github.repo}}"
            },
            {
              "name": "GitHub Issues",
              "icon":
                "https://img.shields.io/github/issues/{{github.user}}/{{github.repo}}.svg?style=flat-square",
              "url": "https://github.com/{{github.user}}/{{github.repo}}/issues"
            },
            {
              "name": "Stories in Ready",
              "icon":
                "https://badge.waffle.io/{{github.user}}/{{github.repo}}.svg?label=ready&title=Ready",
              "url": "http://waffle.io/{{github.user}}/{{github.repo}}"
            },
            {
              "name": "Dependency Status",
              "icon":
                "https://david-dm.org/{{github.user}}/{{github.repo}}.svg",
              "url": "https://david-dm.org/{{github.user}}/{{github.repo}}"
            },
            {
              "name": "devDependency Status",
              "icon":
                "https://david-dm.org/{{github.user}}/{{github.repo}}/dev-status.svg",
              "url":
                "https://david-dm.org/{{github.user}}/{{github.repo}}#info=devDependencies"
            },
            {
              "name": "docs",
              "icon":
                "http://inch-ci.org/github/{{github.user}}/{{github.repo}}.svg?branch=master",
              "url": "http://inch-ci.org/github/{{github.user}}/{{github.repo}}"
            },
            {
              "name": "XO code style",
              "icon": "https://img.shields.io/badge/code_style-XO-5ed9c7.svg",
              "url": "https://github.com/sindresorhus/xo"
            },
            {
              "name": "downloads",
              "icon":
                "http://img.shields.io/npm/dm/{{github.repo}}.svg?style=flat-square",
              "url": "https://npmjs.org/package/{{github.repo}}"
            },
            {
              "name": "Commitizen friendly",
              "icon":
                "https://img.shields.io/badge/commitizen-friendly-brightgreen.svg",
              "url": "http://commitizen.github.io/cz-cli/"
            }
          ]
        }
      },
      {
        "merger": "Rollup",
        "pattern": "**/rollup.config.js"
      },
      {
        "merger": "License",
        "pattern": "LICENSE"
      },
      {
        "merger": "MergeAndRemoveLineSet",
        "pattern": ".gitignore",
        "options": {
          "message": "chore(git): update {{path}} from template"
        }
      },
      {
        "merger": "MergeAndRemoveLineSet",
        "pattern": ".npmignore",
        "options": {
          "message": "chore(npm): update {{path}} from template"
        }
      },
      {
        "merger": "ReplaceIfEmpty",
        "pattern": "**/*"
      }
    ],
    "usedBy": [
      "Kronos-Integration/interceptor-gzip",
      "Kronos-Integration/kronos-cluster-node",
      "Kronos-Integration/kronos-endpoint",
      "Kronos-Integration/kronos-flow",
      "Kronos-Integration/kronos-flow-control-step",
      "Kronos-Integration/kronos-http-routing-step",
      "Kronos-Integration/kronos-interceptor",
      "Kronos-Integration/kronos-interceptor-decode-json",
      "Kronos-Integration/kronos-interceptor-http-request",
      "Kronos-Integration/kronos-interceptor-message-handler",
      "Kronos-Integration/kronos-interceptor-stream-obj2string",
      "Kronos-Integration/kronos-koa",
      "Kronos-Integration/kronos-service",
      "Kronos-Integration/kronos-service-admin",
      "Kronos-Integration/kronos-service-consul",
      "Kronos-Integration/kronos-service-health-check",
      "Kronos-Integration/kronos-service-koa",
      "Kronos-Integration/kronos-service-logger-gelf",
      "Kronos-Integration/kronos-service-manager",
      "Kronos-Integration/kronos-service-registry",
      "Kronos-Integration/kronos-service-uti",
      "Kronos-Integration/kronos-step",
      "Kronos-Integration/kronos-step-aggregate",
      "Kronos-Integration/kronos-step-archive-arangodb",
      "Kronos-Integration/kronos-step-archive-tar",
      "Kronos-Integration/kronos-step-passthrough",
      "Kronos-Integration/kronos-step-stdio",
      "Kronos-Integration/kronos-step-system",
      "Kronos-Integration/test-interceptor",
      "Kronos-Integration/test-step",
      "arlac77/aggregation-repository-provider",
      "arlac77/bitbucket-repository-provider",
      "arlac77/bocf",
      "arlac77/cache-repository-provider",
      "arlac77/config-expander",
      "arlac77/config-expander-cli",
      "arlac77/es6-modules-playground",
      "arlac77/expression-expander",
      "arlac77/fs-resolver-fs",
      "arlac77/gd-playground",
      "arlac77/gitea-repository-provider",
      "arlac77/github-repository-provider",
      "arlac77/kv-reader",
      "arlac77/local-repository-provider",
      "arlac77/log-stream-analyser",
      "arlac77/loglevel-mixin",
      "arlac77/mock-repository-provider",
      "arlac77/model",
      "arlac77/model-attributes",
      "arlac77/mqtt-timeseries-leveldb",
      "arlac77/named-conf",
      "arlac77/node-symatem",
      "arlac77/npm-navigator",
      "arlac77/npm-package-walker",
      "arlac77/npm-template-sync",
      "arlac77/npm-template-sync-github-hook",
      "arlac77/pratt-parser",
      "arlac77/repository-provider",
      "arlac77/rpm-codec",
      "arlac77/rtsp-archive",
      "arlac77/scope-reporter",
      "arlac77/sftp-resolver-fs",
      "arlac77/statetransition-mixin",
      "arlac77/svn-dav-fs",
      "arlac77/svn-repository-provider",
      "arlac77/svn-simple-auth-provider",
      "arlac77/symatem-infrastructure",
      "arlac77/timeseries-sqlite2leveldb",
      "arlac77/chunk-tokenizer",
      "arlac77/url-cmd",
      "arlac77/url-resolver-fs",
      "arlac77/uti"
    ]
  }
}
