{
  "name": "{{name}}",
  "version": "0.0.0-semantic-release",
  "description": "{{description}}",
  "module": "{{module}}",
  "main": "{{main}}",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "npm-package-template"
  ],
  "scripts": {
    "cover": "nyc --temp-directory build/nyc ava",
    "precover": "rollup -c tests/rollup.config.js",
    "test": "ava",
    "pretest": "rollup -c tests/rollup.config.js",
    "posttest": "npm run prepare && markdown-doctest",
    "prepare": "rollup -c",
    "docs": "documentation readme {{module}} --section=API",
    "lint": "documentation lint {{module}}",
    "semantic-release": "semantic-release",
    "travis-deploy-once": "travis-deploy-once"
  },
  "dependencies": {},
  "devDependencies": {
    "jsdoc-babel": "-",
    "jsdoc-to-markdown": "-",
    "jsdoc": "-",
    "istanbul": "-",
    "mocha": "-",
    "chai": "-",
    "xo": "-",
    "cz-conventional-changelog": "-",
    "babel-plugin-transform-async-generator-functions": "-",
    "mocha-lcov-reporter": "-",
    "documentation": "^8.0.0",
    "markdown-doctest": "^0.9.1",
    "nyc": "^12.0.2",
    "ava": "^0.25.0",
    "rollup": "^0.60.2",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-babel": "^3.0.4",
    "rollup-plugin-json": "^3.0.0",
    "rollup-plugin-multi-entry": "^2.0.2",
    "rollup-plugin-executable": "^1.2.0",
    "babel-preset-env": "^1.6.1",
    "semantic-release": "^15.5.2",
    "travis-deploy-once": "^5.0.0"
  },
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "engines": {
    "node": ">=8.11"
  },
  "xo": {
    "space": true
  },
  "ava": {
    "files": [
      "build/*-test.js"
    ],
    "presets": [
      "env"
    ],
    "require": [
      "babel-register"
    ]
  },
  "nyc": {
    "include": [
      "build/*-test.js"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "report-dir": "./build/coverage"
  },
  "template": {
    "usedBy": [
      "arlac77/npm-navigator"
    ]
  }
}